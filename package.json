{
	"name": "lambdaorm-svc",
	"version": "0.0.1",
	"description": "service of lambda orm",
	"main": "./dist/lib/index",
	"scripts": {
		"start": "node build/lib/index.js",
		"predev": "npm run swagger",
		"prebuild": "npm run swagger",
		"build": "npx tsc",
		"dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
		"swagger": "tsoa spec"
	},
	"nodemonConfig": {
		"watch": [
			"src"
		],
		"ext": "ts",
		"exec": "ts-node src/lib/index.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FlavioLionelRita/lambdaorm-svc.git"
	},
	"keywords": [
		"orm",
		"lambdaorm",
		"lambdaorm-svc",
		"service",
		"builder",
		"expression",
		"parser",
		"database",
		"data access"
	],
	"author": "Flavio Lionel Rita <flaviolrita@hotmail.com>",
	"license": "Apache-2.0 License",
	"bugs": {
		"url": "https://github.com/FlavioLionelRita/lambdaorm-svc/issues"
	},
	"homepage": "https://github.com/FlavioLionelRita/lambdaorm-svc#readme",
	"dependencies": {
		"express": "^4.17.1",
		"lambdaorm": "0.1.23-alpha",
		"morgan": "^1.10.0",
		"reflect-metadata": "^0.1.13",
		"swagger-ui-express": "^4.1.6",
		"tsoa": "^3.14.1",
		"mongodb": "^4.1.3",
		"mysql2": "^2.2.5",
		"pg": "^8.7.1",
		"tedious": "^14.1.0",
		"sql.js": "^1.6.2"
	},
	"devDependencies": {
		"@babel/eslint-parser": "^7.15.4",
		"@types/express": "^4.17.12",
		"@types/morgan": "^1.9.2",
		"@types/mysql": "^2.15.18",
		"@types/node": "^15.12.2",
		"@types/swagger-ui-express": "^4.1.2",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"concurrently": "^6.2.0",
		"config-extends": "^1.0.4",
		"dotenv": "^10.0.0",
		"eslint": "^7.32.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.24.2",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-promise": "^5.1.0",
		"nodemon": "^2.0.7",
		"ts-node": "^10.0.0",
		"typescript": "^4.3.5"
	}
}