	"version": "0.2.0",
	"configurations": [
		{
			"name": "nordwind api",
			"type": "node",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/src/api/index.ts",
			"envFile": "${workspaceFolder}/src/dev/lab/nordwind/.env"
		},
		{
			"name": "nordwind openapi",
			"type": "node",
			"request": "launch",
